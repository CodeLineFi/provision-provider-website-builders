{
    "name": "upmind/provision-provider-website-builders",
    "description": "This provision category contains the common functions used in provisioning flows for website builders and related tools",
    "type": "library",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Harry Lewis",
            "email": "harry@upmind.com"
        }
    ],
    "require": {
        "php": "8.1 - 8.3",
        "upmind/provision-provider-base": "^4.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Upmind\\ProvisionProviders\\WebsiteBuilders\\": "src/"
        }
